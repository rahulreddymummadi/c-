#include <iostream>

int main() {
    int age;
    std::cout << "Enter your age: ";
    std::cin >> age;

    const int votingAge = 18; // Minimum voting age

    if (age >= votingAge) {
        std::cout << "You are eligible to vote." << std::endl;
    } else {
        std::cout << "You are not eligible to vote yet." << std::endl;
    }

    return 0;
}
